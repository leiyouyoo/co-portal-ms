{
    "name": "co-portal",
    "version": "0.0.1",
    "description": "微服务门户项目",
    "author": "cityocean",
    "repository": {
        "type": "git",
        "url": "git+https://icp.cityocean.com"
    },
    "homepage": "https://icp.cityocean.com",
    "scripts": {
        "start": "concurrently \"npm run serve:portal\" \"npm run serve:fcm\" \"npm run serve:platform\" \"npm run serve:frm\"  \"npm run serve:crm\" \"npm run serve:fam\" \"npm run serve:wf\" \"npm run serve:pub\" \"npm run serve:csp\"",
        "serve:portal": "ng run portal:serve --port 3000",
        "build:portal:dev": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng run portal:build",
        "build:portal:prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build portal --prod",
        "sonar:portal": "sonar-scanner",
        "analyze:portal": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build fcm --prod --stats-json",
        "start:platform": "concurrently \"npm run serve:portal\" \"npm run serve:platform\"",
        "serve:platform": "ng run platform:serve --deploy-url=/apps/platform/ --port 3001",
        "serve:platform:dev": "ng run platform-dev:serve --deploy-url=/apps/platform/",
        "build:platform:dev": "ng run platform:build --deploy-url=/apps/platform/",
        "build:platform:prod": "ng build platform --prod --deploy-url=/apps/platform/",
        "sonar:platform": "cd ./apps/platform/ && sonar-scanner -Dproject.settings=sonar.properties",
        "analyze:platform": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build platform --prod --stats-json",
        "start:fcm": "concurrently \"npm run serve:portal\" \"npm run serve:fcm\"",
