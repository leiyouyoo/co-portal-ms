        "build:pub:prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build pub --prod --deploy-url=/apps/pub/",
        "sonar:pub": "cd ./apps/pub/ && sonar-scanner -Dproject.settings=sonar.properties",
        "analyze:pub": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build pub --prod --stats-json",
        "start:csp": "concurrently \"ng run portal-csp:serve --port 3000\" \"npm run serve:csp\"",
        "serve:csp": "ng run csp:serve --deploy-url=/apps/csp/ --port 3008",
        "serve:csp:dev": "ng run csp-dev:serve --deploy-url=/apps/csp/",
        "build:csp:dev": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build  csp --deploy-url=/apps/csp/",
        "build:csp:prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build csp --prod --deploy-url=/apps/csp/",
        "sonar:csp": "cd ./apps/csp/ && sonar-scanner -Dproject.settings=sonar.properties",
        "analyze:csp": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build csp --prod --stats-json",
        "lint": "npm run lint:ts && npm run lint:style",
        "lint:ts": "ng lint --fix",
        "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
        "lint-staged": "lint-staged",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "e2e": "ng e2e",
        "test": "ng test --watch",
        "test-coverage": "ng test --code-coverage --watch=false",
        "theme": "node scripts/theme.js",
        "rm:env": "rimraf dist/portal/platform.config.js",
        "update:co": "npm i @co/cds@latest @co/acl@latest  @co/auth@latest @co/cache@latest @co/cbc@latest @co/chart@latest @co/common@latest @co/cms@latest @co/core@latest @co/form@latest @co/im@latest @co/mock@latest @co/theme@latest co-cli@latest",
        "clear:co": "npm uninstall @co/cds @co/acl  @co/auth @co/cache @co/cbc @co/chart @co/common @co/cms @co/core @co/form @co/im @co/mock @co/theme co-cli && rimraf node_modules/@co",
