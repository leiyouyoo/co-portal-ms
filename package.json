{
  "name": "ng-alain-pro",
  "version": "0.0.0",
  "description": "Ng-alain business theme, ng-zorro-antd admin panel front-end framework",
  "author": "cityocean",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-alain/ng-alain.git"
  },
  "homepage": "https://e.ng-alain.com/theme/pro",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng serve --port 6200",
    "hmr": "ng serve -c=hmr",
    "start:ms": "concurrently \"npm run serve:portal\" \"npm run serve:app3\"",
    "//start:ms": "concurrently \"npm run serve:portal\" \"npm run serve:app3\"",
    "serve:portal": "ng run portal:serve",
    "serve:app3": "ng serve app3 --deploy-url=/static/app3/",
    "serve:app2": "ng serve app2 --deploy-url=/static/app2/",
    "build": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod",
    "build:dev": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod -c dev",
    "build:test": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod -c test",
    "analyze": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "theme": "node scripts/theme.js",
    "icon": "ng g ng-alain:plugin icon",
    "update:co": "npm i @co/cds@latest @co/acl@latest @co/acl@latest @co/auth@latest @co/cache@latest @co/cbc@latest @co/chart@latest @co/common@latest @co/core@latest @co/form@latest @co/mock@latest @co/theme@latest co-cli@latest"
  },
