{
  "name": "co-portal",
  "version": "0.0.1",
  "description": "微服务门户项目",
  "author": "cityocean",
  "repository": {
    "type": "git",
    "url": "git+https://icp.cityocean.com"
  },
  "homepage": "https://icp.cityocean.com",
  "scripts": {
    "start": "concurrently \"npm run serve:portal\" \"npm run serve:platform\" \"npm run serve:fcm\" \"npm run serve:frm\"  \"npm run serve:crm\"",
    "start:ms": "concurrently \"npm run serve:portal\" \"npm run serve:platform\"",
    "start:fcm": "concurrently \"npm run serve:portal\" \"npm run serve:fcm\"",
    "start:frm": "concurrently \"npm run serve:portal\" \"npm run serve:frm\"",
    "start:crm": "concurrently \"npm run serve:portal\" \"npm run serve:crm\"",
    "serve:portal": "ng run portal:serve --port 3000",
    "build:portal:dev": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng run portal:build",
    "build:portal:prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build portal --prod",
    "analyze:portal": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build fcm --prod --stats-json",
    "sonar:portal": "sonar-scanner",
    "serve:crm": "ng run crm:serve --deploy-url=/apps/crm/ --port 3003",
    "serve:frm": "ng run frm:serve --deploy-url=/apps/frm/ --port 3004",
    "serve:platform": "ng run platform:serve --deploy-url=/apps/platform/ --port 3001",
    "serve:platform:dev": "ng run platform-dev:serve --deploy-url=/apps/platform/",
    "build:platform:dev": "ng run platform:build --dep·loy-url=/apps/platform/",
    "build:platform:prod": "ng build platform --prod --deploy-url=/apps/platform/",
    "analyze:platform": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build platform --prod --stats-json",
    "sonar:platform": "cd ./apps/platform/ && sonar-scanner -Dproject.settings=sonar.properties",
    "serve:fcm": "ng run fcm:serve --deploy-url=/apps/fcm/ --port 3002",
    "serve:fcm:dev": "ng run fcm-dev:serve --deploy-url=/apps/fcm/",
    "build:fcm:dev": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build  fcm --deploy-url=/apps/fcm/",
    "analyze:fcm": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build fcm --prod --stats-json",
    "sonar:fcm": "cd ./apps/fcm/ && sonar-scanner -Dproject.settings=sonar.properties",
    "build:fcm:prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build fcm --prod --deploy-url=/apps/fcm/",
    "build:crm:prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build crm --prod --deploy-url=/apps/crm/",
    "build:frm:prod": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build frm --prod --deploy-url=/apps/frm/",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "rm:env": "rimraf dist/portal/platform.config.js",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "theme": "node scripts/theme.js",
    "update:co": "npm i @co/cds@latest @co/acl@latest  @co/auth@latest @co/cache@latest @co/cbc@latest @co/chart@latest @co/common@latest @co/cms@latest @co/core@latest @co/form@latest @co/mock@latest @co/theme@latest co-cli@latest",
    "clear:co": "npm uninstall @co/cds @co/acl  @co/auth @co/cache @co/cbc @co/chart @co/common @co/cms @co/core @co/form @co/mock @co/theme co-cli && rimraf node_modules/@co",
    "ngcc": "ngcc"
  },
